{
	"name": "newshacker",
	"version": "0.2",
	"description": "Fixes fluid width on hacker news.",
	"content_scripts": [{
		"matches": ["https://news.ycombinator.com/*"],
		"js": ["jquery.min.js", "newshacker.js"],
		"css": ["newshacker.css"],
		"run_at": "document_end"
	}],
	"permissions": ["<all_urls>"],
	"manifest_version": 2
}